FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default                         none;
    div(phi,U)                      Gauss limitedLinearV 1;
    div(phi,e)                      Gauss limitedLinear01 1;
    div(phi,h)                      Gauss limitedLinear01 1;
    div(phi,K)                      Gauss limitedLinear01 1;
    div(phi,(p|rho))                Gauss limitedLinear01 1;
    div(phi,k)                      Gauss limitedLinear01 1;
    div(phi,epsilon)               Gauss limitedLinear01 1;
    div(((rho*nuEff)*dev2(T(grad(U))))) Gauss limitedLinear01 1;
    div(phi,CO2) Gauss limitedLinear01 1;
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}

wallDist
{
    method meshWave;
}


// ************************************************************************* //
