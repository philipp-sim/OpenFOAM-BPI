FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "${FOAM_CASE}/constant/initialisationConditions"


regions
(
    boxToCell
    {
        box (-4.15 -10.5 0) (5.85 -0.1 10);
        fieldValues
        (
            volScalarFieldValue T $!atmosphere/T
            volScalarFieldValue CO2 $!atmosphere/CO2
            volScalarFieldValue Y_CO2 $!atmosphere/Y_CO2
            volScalarFieldValue specificHumidity $!atmosphere/specificHumidity
        );
    }
    boxToCell
    {
        box (-4.15 6.1 0) (5.85 16.2 10);
        fieldValues
        (
            volScalarFieldValue T $!atmosphere/T
            volScalarFieldValue CO2 $!atmosphere/CO2
            volScalarFieldValue Y_CO2 $!atmosphere/Y_CO2
            volScalarFieldValue specificHumidity $!atmosphere/specificHumidity
        );
    }


);


// ************************************************************************* //
