FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //



dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0.00003971;

bcField uniform 0.00000000001;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    atmosphere
    {
	type            calculated;
        value           $bcField; //$internalField;
    }

    staticWalls
    {
        
	type            compressible::alphatJayatillekeWallFunction;//compressible::alphatWallFunction;
        Prt            	0.85;
        value           $bcField; //$internalField;
    }
	
    movingWalls
    {
	$staticWalls;
    }

    nonCouple
    {
        type            zeroGradient;
    }

    ".*"
    {
        type            zeroGradient;
    }
}

// ************************************************************************* //
