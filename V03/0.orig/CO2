FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      CO2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "${FOAM_CASE}/constant/initialisationConditions"

dimensions      [0 0 0 0 0 0 0];

internalField   uniform $!internalField/CO2;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"
    
    atmosphere
    {
        type  inletOutlet;
        inletValue uniform $!atmosphere/CO2;
        inlet	    	uniform $!atmosphere/CO2;
    }

    staticWalls
    {
        type            zeroGradient;
    }

    movingWalls
    {
    $staticWalls;
    }

    nonCouple
    {
        type            zeroGradient;
    }
    
    ".*"
    {
        type            zeroGradient;
    }
}

// ************************************************************************* //
